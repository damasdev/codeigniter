</html></div><script src="{{ base_url ('/js/app.min.js')}}"></script><script src="{{ base_url ('/vendor/jquery/jquery-3.6.0.min.js')}}"></script><script src="{{ base_url ('/vendor/sweetalert/sweetalert2.all.min.js')}}"></script>{{ block("scripts") }}</body></html>
